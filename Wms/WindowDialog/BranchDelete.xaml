<Window x:Class="Wms.WindowDialog.BranchDelete"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Height="100"
        Width="300"
        WindowStartupLocation="CenterScreen"
        WindowStyle="None"
        ResizeMode="NoResize" Background="DarkGray">
    <StackPanel VerticalAlignment="Center">
        <Label Content="Вы действительно хотите удалить филиал ?" HorizontalAlignment="Center"></Label>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0 10 0 0">
            <Button Click="Close" Width="60" Height="20" Content="Да" Command="{Binding Path=DeleteCommand}"/>
            <Button Click="Close" Width="60" Height="20" Content="Нет" Margin="10 0 0 0"/>
        </StackPanel>
    </StackPanel>
</Window>
