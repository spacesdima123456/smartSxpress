<window:DisplayAlertBase xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"  
        Width="250"
        Height="190"
        mc:Ignorable="d"
        Title="Настройки"
        ResizeMode="NoResize"
        xmlns:window="clr-namespace:Wms.View.WindowDialog"
        x:Class="Wms.View.WindowDialog.DisplayAlertSettings"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006">
    <StackPanel >
        <StackPanel Width="200" Height="50">
            <Label Content="Принтеры"/>
            <dxe:ComboBoxEdit />
        </StackPanel>
        <StackPanel Width="200" Height="50">
            <Label Content="Документы"/>
            <dxe:ComboBoxEdit />
        </StackPanel>
        <Button Content="Сохранить"  Width="200"/>
    </StackPanel>
</window:DisplayAlertBase>
